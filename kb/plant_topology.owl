</owl:Class>

<owl:NamedIndividual rdf:about="#Pump_01">
  <rdf:type rdf:resource="#Component"/>
  <connectedTo rdf:resource="#Pipe_01"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Pipe_01">
  <rdf:type rdf:resource="#Component"/>
  <connectedTo rdf:resource="#Tank_01"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tank_01">
  <rdf:type rdf:resource="#Component"/>
</owl:NamedIndividual>


</rdf:RDF>
